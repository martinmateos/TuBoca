<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TuBoca.Paginas.Test">
    <StackLayout BindingContext="">
    
        <ListView x:Name="listViewEnunciado" >
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Data}"  />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView> 
        <Label x:Name="enunciadoLabel" Text="{Binding}" LineBreakMode="CharacterWrap">
        </Label>
        <Button x:Name="btnNext" Clicked="btnNext_Clicked"/>
    </StackLayout>
</ContentPage>